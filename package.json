{
  "name": "ba",
  "version": "5.0.0",
  "description": "reacts to discord messages with fun emojis",
  "main": "dist/src/index.js",
  "dependencies": {
    "discord.js": "discordjs/discord.js",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.29",
    "typeorm-seeding": "^1.6.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.0",
    "@types/faker": "^5.1.5",
    "@types/node": "^14.11.10",
    "dotenv": "^8.2.0",
    "ts-standard": "^10.0.0",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "build": "tsc",
    "lint": "ts-standard",
    "lint:fix": "ts-standard --fix",
    "seed": "tsc && node -r dotenv/config ./node_modules/typeorm-seeding/dist/cli.js seed",
    "start": "tsc && node -r dotenv/config .",
    "typeorm": "tsc && node -r dotenv/config ./node_modules/typeorm/cli.js"
  },
  "ts-standard": {
    "report": "stylish"
  }
}
